{"ast":null,"code":"export default {\n  name: \"chouKa\",\n\n  data() {\n    return {\n      diamond: \"111111111111\",\n      msg: \"小熊\",\n      picture: \"\",\n      imgWrap: [{\n        url: require(\"../assets/5.jpg\")\n      }, {\n        url: require(\"../assets/4.jpg\")\n      }, {\n        url: require(\"../assets/3.png\")\n      }, {\n        url: require(\"../assets/2.png\")\n      }, {\n        url: require(\"../assets/1.jpg\")\n      }],\n      ur: []\n    };\n  },\n\n  mounted() {\n    fetch('/api/crystal').then(res => {\n      res.json().then(response => {\n        this.diamond = response;\n      });\n    });\n  },\n\n  methods: {\n    recharge() {\n      this.diamond -= 1000;\n    },\n\n    show01() {\n      document.getElementById(\"modal-background\").style.display = \"flex\";\n      fetch('api/single').then(response => response.json()).then(res => {\n        this.ur[0] = res;\n      });\n    },\n\n    show10() {\n      document.getElementById(\"modal-background\").style.display = \"flex\";\n      fetch('api/tencards').then(response => response.json()).then(res => {\n        this.ur = res;\n      });\n    },\n\n    nice() {\n      document.getElementById(\"modal-background\").style.display = \"none\";\n      this.ur = [];\n    },\n\n    logout() {\n      this.$router.push({\n        \"name\": \"mainInterface\"\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0DA,eAAe;EACbA,IAAI,EAAE,QADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,cADJ;MAELC,GAAG,EAAE,IAFA;MAGLC,OAAO,EAAE,EAHJ;MAILC,OAAO,EAAE,CACP;QAACC,GAAG,EAAEC,OAAO,CAAC,iBAAD;MAAb,CADO,EAEP;QAACD,GAAG,EAAEC,OAAO,CAAC,iBAAD;MAAb,CAFO,EAGP;QAACD,GAAG,EAAEC,OAAO,CAAC,iBAAD;MAAb,CAHO,EAIP;QAACD,GAAG,EAAEC,OAAO,CAAC,iBAAD;MAAb,CAJO,EAKP;QAACD,GAAG,EAAEC,OAAO,CAAC,iBAAD;MAAb,CALO,CAJJ;MAWLC,EAAE,EAAE;IAXC,CAAP;EAaD,CAhBY;;EAiBbC,OAAO,GAAG;IACRC,KAAK,CAAC,cAAD,CAAL,CACKC,IADL,CACWC,GAAD,IAAS;MACbA,GAAG,CAACC,IAAJ,GAAWF,IAAX,CAAiBG,QAAD,IAAc;QAC5B,KAAKZ,OAAL,GAAeY,QAAf;MACD,CAFD;IAGD,CALL;